# 测试环境配置
# Test Environment Configuration

# 环境标识
VITE_APP_ENV=test

# API服务器地址 (测试环境)
VITE_API_BASE_URL=https://test-api.weeklyreport.com/api
API_BASE_URL=https://test-api.weeklyreport.com

# 应用标题
VITE_APP_TITLE=WeeklyReport 测试环境

# API配置
VITE_API_TIMEOUT=12000
API_TIMEOUT=36000
RETRY_ATTEMPTS=3
SYNC_INTERVAL=120000

# WebSocket服务器地址
VITE_WS_URL=wss://test-api.weeklyreport.com/ws

# 文件上传地址
VITE_UPLOAD_URL=https://test-api.weeklyreport.com/api/upload

# 静态资源CDN地址
VITE_CDN_URL=https://test-cdn.weeklyreport.com

# 调试配置 (测试环境适中调试)
VITE_DEBUG=true
DEBUG_MODE=true
VITE_LOG_LEVEL=info
LOG_LEVEL=info

# 开发工具 (测试环境开启部分)
VITE_SHOW_DEVTOOLS=true
VITE_HOT_RELOAD=false

# 测试专用配置
VITE_ENABLE_MOCK=true
VITE_TEST_DATA=true
VITE_SKIP_AUTH=false

# 性能监控 (测试环境)
VITE_ENABLE_ANALYTICS=false
VITE_SENTRY_DSN=https://your-test-sentry-dsn@sentry.io/test-project-id

# 安全配置
VITE_SECURE_COOKIES=false
VITE_CSRF_PROTECTION=true