# 生产环境配置
# Production Environment Configuration

# 环境标识
VITE_APP_ENV=production

# API服务器地址 (生产环境)
VITE_API_BASE_URL=https://api.weeklyreport.com/api
API_BASE_URL=https://api.weeklyreport.com

# 应用标题
VITE_APP_TITLE=WeeklyReport

# API配置
VITE_API_TIMEOUT=15000
API_TIMEOUT=45000
RETRY_ATTEMPTS=3
SYNC_INTERVAL=300000

# WebSocket服务器地址
VITE_WS_URL=wss://api.weeklyreport.com/ws

# 文件上传地址
VITE_UPLOAD_URL=https://api.weeklyreport.com/api/upload

# 静态资源CDN地址
VITE_CDN_URL=https://cdn.weeklyreport.com

# 调试配置 (生产环境关闭调试)
VITE_DEBUG=false
DEBUG_MODE=false
VITE_LOG_LEVEL=error
LOG_LEVEL=error

# 开发工具 (生产环境关闭)
VITE_SHOW_DEVTOOLS=false
VITE_HOT_RELOAD=false

# 性能监控
VITE_ENABLE_ANALYTICS=true
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# 安全配置
VITE_SECURE_COOKIES=true
VITE_CSRF_PROTECTION=true